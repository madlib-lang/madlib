// file: /madlib_modules/random/madlib_modules/math/src/Main.mad
const toString = (fn, args = []) => () => (
  `curry(${fn.toString()})${args.length > 0 ? `(${args.join(`,`)})` : ``}`
)
const curryPowder = (fn) => {
  function curried(...args) {
    const length = args.length
    function saucy(...args2) {
      return curried.apply(this, args.concat(args2))
    }
    saucy.toString = toString(fn, args)
    return (
      length >= fn.length ?
      fn.apply(this, args) :
      saucy
    )
  }
  curried.toString = toString(fn)
  return curried
};

export const avg = curryPowder((a, b) => a + b / 2);
export default { avg };
// file: /madlib_modules/random/src/Main.mad
import R from "./Utils/Random.mjs";
import M from "./../.deps/math/src/Main.mjs";

const toString = (fn, args = []) => () => (
  `curry(${fn.toString()})${args.length > 0 ? `(${args.join(`,`)})` : ``}`
)
const curryPowder = (fn) => {
  function curried(...args) {
    const length = args.length
    function saucy(...args2) {
      return curried.apply(this, args.concat(args2))
    }
    saucy.toString = toString(fn, args)
    return (
      length >= fn.length ?
      fn.apply(this, args) :
      saucy
    )
  }
  curried.toString = toString(fn)
  return curried
};

export const random = curryPowder((seed) => R.random(seed) + M.avg(seed, seed));
export default { random };
// file: /madlib_modules/random/src/Utils/Random.mad
