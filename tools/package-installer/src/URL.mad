import { mapChars } from "String"



replaceSpecialChar :: String -> String
replaceSpecialChar = where {
  ":" => "_"
  "/" => "_"
  "#" => "_"
  "." => "_"
  "?" => "_"
  "&" => "_"
  "=" => "_"
  "-" => "_"
  char => char
}


sanitizeUrl :: String -> String
export sanitizeUrl = mapChars(replaceSpecialChar)
