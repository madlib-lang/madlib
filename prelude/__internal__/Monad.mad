import {} from "Applicative"



interface Applicative m => Monad m {
  chain :: (a -> m b) -> m a -> m b

  of :: a -> m a
}


chain2 :: Monad m => (a -> b -> m c) -> m a -> m b -> m c
chain2 = (f, ma, mb) => chain((a) => chain((b) => f(a, b), mb), ma)


andDo :: Monad m => m b -> m a -> m b
export andDo = (b, a) => chain((_) => b, a)
